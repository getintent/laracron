{
  "scheduledJobs": {
    "everyMinute": [
      "app/console list",
      "app/console"
    ],
    "*/10 * * * *": [
      "echo '*/10 * * * *'"
    ]
  },

  "log file": "php://stdout",
  "basePath": ".",
  "cache.default": "file",
  "cache.prefix": "laracron",
  "cache.stores.file": {
    "driver": "file",
    "path": "cache"
  },
  "cache.stores.redis": {
    "driver": "redis"
  }
}
